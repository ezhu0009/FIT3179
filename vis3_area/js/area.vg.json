
{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",

    "title": {
          "text": "Annual number of Attacks from 1791 - 2024",
          "anchor": "middle",
          "fontSize": 18
        },

    "width": 450,
    "height": 300,

    "data": {"url": "https://raw.githubusercontent.com/ezhu0009/FIT3179/refs/heads/main/vis3_area/data/yearly_shark.csv"},
    "mark": "area",




      


    "encoding": {
      "x": {
        "timeUnit": "yearmonth", "field": "year",
        "axis": {"format": "%Y"},
        "title": "Year"
      },

      "y": {
        "aggregate": "sum", "field": "number_incidents",
        "title": "Number of Attacks"
      }
    }

  }
